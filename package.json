{
  "name": "vue-popperjs",
  "version": "3.0.0",
  "type": "module",
  "description": "VueJS popover",
  "main": "dist/vue-popper.js",
  "module": "dist/vue-popper.umd.cjs",
  "jsnext:main": "src/index.js",
  "unpkg": "dist/vue-popper.min.js",
  "exports": {
    ".": {
      "import": "./dist/vue-popper.js",
      "require": "./dist/vue-popper.umd.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite serve",
    "lint": "eslint ./",
    "build": "vite build",
    "postinstall": "opencollective-postinstall || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinCK/vue-popper.git"
  },
  "author": "Igor Ognichenko <ognichenko.igor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobinCK/vue-popper/issues"
  },
  "keywords": [
    "vue-popper.js",
    "vue-popover",
    "vuejs-popper.js",
    "vuejs-popover",
    "vue",
    "vuejs",
    "vue-plugin",
    "vue-component"
  ],
  "homepage": "https://github.com/RobinCK/vue-popper#readme",
  "semistandard": {
    "ignore": [
      "node_modules",
      "bower_components",
      "build",
      "dist",
      "test"
    ]
  },
  "dependencies": {
    "opencollective-postinstall": "^2.0.2",
    "popper.js": "^1.15.0",
    "vite": "^4.2.1",
    "vue-demi": "^0.14.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^5.16.0",
    "terser": "^5.16.9",
    "vue": "^3.1.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/vue-popper"
  }
}
